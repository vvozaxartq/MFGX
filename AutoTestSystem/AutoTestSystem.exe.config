<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="System.Memory" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-4.0.1.2" newVersion="4.0.1.2" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
	<log4net>
		<appender name="FileAppender" type="log4net.Appender.RollingFileAppender">
			<!-- 使用日期模式添加時間戳到檔案名稱 -->
			<file value="log\" />
			<appendToFile value="true" />
			<staticlogfilename value="false" />
			<rollingStyle value="Date" />
			<datePattern value="yyyyMMdd 'log.txt'" />
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss} [%level] - %message%newline" />
			</layout>
		</appender>
		<appender name="RecipeSettingFileAppender" type="log4net.Appender.RollingFileAppender">
			<!-- 使用日期模式添加時間戳到檔案名稱 -->
			<file value="log\RecipeSetting\" />
			<appendToFile value="true" />
			<staticlogfilename value="false" />
			<rollingStyle value="Date" />
			<datePattern value="yyyyMMdd 'log.txt'" />
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss} - %message%newline" />
			</layout>
		</appender>
		<appender name="EquipmentFileAppender" type="log4net.Appender.RollingFileAppender">
			<!-- 使用日期模式添加時間戳到檔案名稱 -->
			<file value="log\Equipment\" />
			<appendToFile value="true" />
			<staticlogfilename value="false" />
			<rollingStyle value="Date" />
			<datePattern value="yyyy-MM-dd' EQ_Log.txt'" />
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss} [%level] %message%newline" />
			</layout>
		</appender>
		<appender name="ConfigurationFileAppender" type="log4net.Appender.RollingFileAppender">
			<!-- 使用日期模式添加時間戳到檔案名稱 -->
			<file value="log\Configuration\" />
			<appendToFile value="true" />
			<staticlogfilename value="false" />
			<rollingStyle value="Date" />
			<datePattern value="yyyy-MM-dd' Config_Log.txt'" />
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss} [%level] [Configuration] %message%newline" />
			</layout>
		</appender>
		<appender name="RichTextBoxAppender" type="AutoTestSystem.BLL.RichTextBoxAppender, AutoTestSystem">
			<FormName>MainForm</FormName>
			<RichTextBoxName>richTextBox1</RichTextBoxName>
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss.fff} [%level] - %message%newline" />
			</layout>
		</appender>
		<appender name="MainRichTextBoxAppender" type="AutoTestSystem.BLL.RichTextBoxAppender, AutoTestSystem">
			<FormName>MainForm</FormName>
			<RichTextBoxName>MainlogRichTextBox</RichTextBoxName>
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss.fff} %message%newline" />
			</layout>
		</appender>
		<appender name="DUTRichTextBoxAppender" type="AutoTestSystem.BLL.RichTextBoxAppender, AutoTestSystem">
			<FormName>MainForm</FormName>
			<RichTextBoxName>DUTRichTextBox</RichTextBoxName>
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss.fff} %message%newline" />
			</layout>
		</appender>
		<appender name="EquipmentRichTextBoxAppender" type="AutoTestSystem.BLL.RichTextBoxAppender, AutoTestSystem">
			<FormName>MainForm</FormName>
			<RichTextBoxName>EQRichTextBox</RichTextBoxName>
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss.fff} [%level] [Equipment] %message%newline" />
			</layout>
		</appender>
		<appender name="RecipeSettingRichTextBoxAppender" type="AutoTestSystem.BLL.RichTextBoxAppender, AutoTestSystem">
			<FormName>RecipeManagement</FormName>
			<RichTextBoxName>richTextBox_RecipeSetting</RichTextBoxName>
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss.fff} %message%newline" />
			</layout>
		</appender>

		<appender name="DevicesSettingRichTextBoxAppender" type="AutoTestSystem.BLL.RichTextBoxAppender, AutoTestSystem">
			<FormName>RecipeManagement</FormName>
			<RichTextBoxName>richTextBox_DeviceListSetting</RichTextBoxName>
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss.fff} %message%newline" />
			</layout>
		</appender>

		<appender name="RecipeProLogRichTextBoxAppender" type="AutoTestSystem.BLL.RichTextBoxAppender, AutoTestSystem">
			<FormName>RecipeManagement</FormName>
			<RichTextBoxName>richTextBox_RecipeProLog</RichTextBoxName>
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss.fff} %message%newline" />
			</layout>
		</appender>


		<appender name="ConfigRichTextBoxAppender" type="AutoTestSystem.BLL.RichTextBoxAppender, AutoTestSystem">
			<FormName>ConfigurationForm</FormName>
			<RichTextBoxName>ConfigRichTextBox</RichTextBoxName>
			<!-- 注意這裡添加了 <layout> 元素 -->
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date{yyyy-MM-dd hh:mm:ss.fff} [%level] %message%newline" />
			</layout>
		</appender>
		<logger name="EquipmentLogger" additivity="false">
			<!-- 等級分為 ALL DEBUG INFO WARN ERROR FATAL OFF -->
			<level value="ALL" />
			<appender-ref ref="EquipmentFileAppender" />
			<appender-ref ref="EquipmentRichTextBoxAppender" />
			<appender-ref ref="MainRichTextBoxAppender" />
			<appender-ref ref="RecipeProLogRichTextBoxAppender" />
			<appender-ref ref="DevicesSettingRichTextBoxAppender" />
		</logger>
		<logger name="ConfigurationLogger" additivity="false">
			<!-- 等級分為 ALL DEBUG INFO WARN ERROR FATAL OFF -->
			<level value="ALL" />
			<appender-ref ref="ConfigurationFileAppender" />
			<appender-ref ref="ConfigRichTextBoxAppender" />
			<appender-ref ref="RecipeProLogRichTextBoxAppender" />
			<appender-ref ref="DevicesSettingRichTextBoxAppender" />
		</logger>
		<logger name="DUTLogger" additivity="false">
			<!-- 等級分為 ALL DEBUG INFO WARN ERROR FATAL OFF -->
			<level value="ALL" />
			<appender-ref ref="DUTRichTextBoxAppender" />
			<appender-ref ref="RecipeProLogRichTextBoxAppender" />
			<appender-ref ref="MainRichTextBoxAppender" />
		</logger>
		<logger name="RecipeSettingLogger" additivity="false">
			<!-- 等級分為 ALL DEBUG INFO WARN ERROR FATAL OFF -->
			<level value="ALL" />
			<appender-ref ref="RecipeSettingFileAppender" />
			<appender-ref ref="RecipeSettingRichTextBoxAppender" />
		</logger>
		<logger name="MainLogger" additivity="false">
			<!-- 等級分為 ALL DEBUG INFO WARN ERROR FATAL OFF -->
			<level value="ALL" />
			<appender-ref ref="MainRichTextBoxAppender" />
		</logger>
		<root>
			<level value="ALL" />
			<appender-ref ref="FileAppender" />
			<appender-ref ref="RichTextBoxAppender" />
			<appender-ref ref="MainRichTextBoxAppender" />
			<appender-ref ref="RecipeProLogRichTextBoxAppender" />



		</root>
	</log4net>

</configuration>
